                {% endfor %}
            </ul>
            </div>
            <div class="rounded border-2 border-red-900 pl-2 mb-10 break-all">
                <p>Najdłuższy url: {{ audit1Data.data.data.maxLenghtInlinks }} znaków</p>
            </div>
            <div class="rounded border-2 border-red-900 pl-2 mb-10 break-all">
                <p>Linki przyjazne wyszukiwarce: 
                     {% if audit1Data.data.data.friendlyLinks %}
                        tak
                    {% else %}
                        nie</p>
                        <p>Lista nieprzyjaznych linków: 
                    <ul>
                        {% for link in audit1Data.data.data.friendlyLinks %}
                            <li>{{ link }}</li>
                        {% endfor %}
                    </ul>
                    {% endif %}</p> 
            </div>
            <div class="rounded border-2 border-red-900 pl-2 mb-10 break-all">
            <p>Lista linków wychodzących:</p>
            {% if audit1Data.data.data.outLinks == 'false' %}
                brak linków wychodzących
            {% else %}
            <ul>
                {% for link in audit1Data.data.data.outLinks %}
                    <li>{{ link.href }} {% if link.rel is not null %} Rel: {{link.rel}} {% endif %}</li>
                {% endfor %} 
            </ul>
            {% endif %}
            </div>
            <div class="rounded border-2 border-red-900 pl-2 mb-10 break-all">
             <p>Kod Google Analytics: 
             {% if audit1Data.data.data.googleAnalytics == 'false' %}
                brak
            {% else %}
                jest
            <ul>
                {% for code in audit1Data.data.data.googleAnalytics %}
                    <li>{{ code }}</li>
                {% endfor %}
            </ul>
            {% endif %}</p>   
            </div>
        </div>
    </div>

